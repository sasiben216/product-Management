<div style="display: flex;justify-content: center;">
    <% data.forEach(function(ele) { %>
        <div>
            <div class="card" >
                
                <div class="card-1" >
                    
                    
                    <p> <img src="<%=ele.img %>" width="200px"></p>
                    <div>
                       
                   <p><label>Id:</label> <span> <%= ele.id %></span></p> 
                   <p><label>Title:</label><span> <%= ele.title %></span></p> 
                   <p><label>Price:</label><span> <%= ele.price %></span></p>
                   <p><label>Seller:</label><span> <%= ele.seller %></span></p> 
                    
                </div>
                </div>
            </div>
        </div>
        <% }) %>
</div>














    </div>
</div>
